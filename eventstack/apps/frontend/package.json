{
  "name": "@eventstack/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "testu": "vitest run --mode unit --coverage",
    "testi": "vitest run --mode integration --coverage",
    "test:coverage": "vitest run --coverage",
    "test:report": "npm run testu && node tests/Unit_test/scripts/exportCoveragePdf.mjs && npm run testi && node tests/Integration_test/scripts/exportCoveragePdf.mjs"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.3",
    "jsdom": "^27.0.1",
    "puppeteer": "^24.26.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.2",
    "vitest": "^4.0.3"
  }
}
